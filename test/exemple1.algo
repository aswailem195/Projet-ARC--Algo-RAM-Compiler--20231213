// test SI   TQ 2 FONCTION
ALGO pairs( )
VAR i <- 20 ,n <- 3
DEBUT
	TQ i > n FAIRE
	   SI  i%2 = 0 ALORS
            ECRIRE(i)
	   FSI
	   i <- i-1
	FTQ
FIN
ALGO pairi( )
VAR i <- 3 ,n <- 20
DEBUT
	TQ i < n FAIRE
	   SI  i%2 = 0 ALORS
            ECRIRE(i)
	   FSI
	   i <- i+1
	FTQ
FIN

PROGRAMME()
//testcomentair
VAR momo ,toto <- 3
DEBUT
      pairi()
      pairs()


FIN
